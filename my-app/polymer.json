{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-dashboard.html",
    "src/my-patients.html",
    "src/my-visits.html",
    "src/my-account.html",
    "src/search-bar.html",
    "src/time-element.html",
    "src/image-resize.html",
    "dist/image-resize.js",
    "src/my-view2.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "dist/*.js",
    "dist/*.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}